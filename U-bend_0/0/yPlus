FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      yPlus;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           uniform 0.1182145;
    }
    empty
    {
        type            empty;
    }
    detachment1
    {
        type            calculated;
        value           uniform 0.1182145;
    }
    detachment2
    {
        type            calculated;
        value           uniform 0.1182145;
    }
}


